<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

	<definition extends="default" name="vorgang/suchenKarte">
		<put-attribute name="body" value="/WEB-INF/views/vorgang/suchenKarte.jspx"/>
		<put-attribute name="menu_item" type="string" value="menu.vorgaenge"/>
		<put-attribute name="tabmenu_item" type="string" value="vorgang.suchen"/>
	</definition>

	<definition extends="default" name="vorgang/bearbeitenDisabled">
		<put-attribute name="body" value="/WEB-INF/views/vorgang/bearbeitenDisabled.jspx"/>
		<put-attribute name="menu_item" type="string" value="menu.vorgaenge"/>
		<put-attribute name="tabmenu_item" type="string" value="vorgang.bearbeiten"/>
	</definition>

	<definition extends="default" name="vorgang/*">
		<put-attribute name="body" value="/WEB-INF/views/vorgang/{1}.jspx"/>
		<put-attribute name="menu_item" type="string" value="menu.vorgaenge"/>
		<put-attribute name="tabmenu_item" type="string" value="vorgang.{1}"/>
	</definition>
</tiles-definitions>
